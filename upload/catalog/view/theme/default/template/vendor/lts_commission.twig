{{ header }}
<div class="container">
  <ul class="breadcrumb">
    {% for breadcrumb in breadcrumbs %}
      <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
      {% endfor %}
  </ul>
  {% if success %}
    <div class="alert alert-success alert-dismissible"><i class="fa fa-check-circle"></i> {{ success }}</div>
  {% endif %}

  <div class="row">   
    {{ lts_column_left }}
    <div id="content" class="col-sm-9">
      {{ lts_mobile_menu }}   
      <div class="lts-dashboard-wrap">
        <div class="row">
          <div class="col-sm-12 lts-products">
            <div class="panel panel-default">
              <div class="panel-heading clearfix"><span>{{ text_list}}</span>
                <div class="pull-right">
                 {# <a href="{{ add }}" data-toggle="tooltip" title="{{ button_add }}" class="btn btn-primary"><i class="fa fa-plus"></i></a>
                  <button type="submit" form="form-product" formaction="{{ copy }}" data-toggle="tooltip" title="{{ button_copy }}" class="btn btn-default"><i class="fa fa-copy"></i></button>

                  {% if module_lts_vendor_delete_product %}
                    <button type="button" form="form-product" formaction="{{ delete }}" data-toggle="tooltip" title="{{ button_delete }}" class="btn btn-danger" onclick="confirm('{{ text_confirm }}') ? $('#form-product').submit() : false;"><i class="fa fa-trash-o"></i></button>
                    {% endif %}
                  #}
                </div>
              </div>
            </div>

            {# FILTER #}

            <div class="well">
              <div class="row">
                <div class="col-sm-4">
                  <div class="form-group">
                    <label class="control-label" for="input-name">{{ entry_date_start }}</label>
                    <input type="text" name="filter_" value="{{ filter_name }}" placeholder="{{ entry_name }}" id="input-name" class="form-control" />
                  </div>
                
                </div>
                <div class="col-sm-4">
                  <div class="form-group">
                    <label class="control-label" for="input-price">{{ entry_date_end }}</label>
                    <input type="text" name="filter_price" value="{{ filter_price }}" placeholder="{{ entry_price }}" id="input-price" class="form-control" />
                  </div>

                   <div class="form-group text-right">
                    <button type="button" id="button-filter" class="btn btn-primary"><i class="fa fa-filter"></i> {{ button_filter }}</button>
                  </div>


                </div>
              </div>
            </div>

            {# Commission list #}


              <div class="table-responsive">
              <table class="table table-bordered table-hover">
                <thead>
                <tr>

                  <th class="text-right">
                    <a href="#">{{ column_order_id }}</a>
                  </th>

                  <th class="text-right">
                    <a href="">{{ column_product }}</a>
                  </th>
                  <th class="text-right"> 
                    <a href="">column_price</a>
                  </th>
                  <th class="text-right">
                    <a href="">{{ column_quantity }}</a>
                    </th>
                  <th class="text-right">
                   <a href="">{{ column_total }}</a>
                  </th>
                  <th class="text-right">
                    <a href="">{{ column_ammount }}</a>
                    </th>
                  <th class="text-right">              
                    <a href="">{{ column_date_added }}</a>
                    </th>
                </tr>
                </thead>
                <tbody>
                  <tr>
                    <td class="text-right">152</td>
                    <td class="text-right">iPhone</td>
                    <td class="text-right">$100.00</td>
                    <td class="text-right">1</td>
                    <td class="text-right">$100.00</td>
                    <td class="text-right">$0.00</td>
                    <td class="text-right">21/02/2020</td>
                  </tr>
                      
                </tbody>
              </table>
            </div>



            <div class="row">
              <div class="col-sm-6 text-left">{{ pagination }}</div>
              <div class="col-sm-6 text-right">{{ results }}</div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
